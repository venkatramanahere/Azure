{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactory-Battula"
		},
		"ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureBlobStorageMovies_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageMovies'"
		},
		"AzureSqlDWLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDWLinkedService'"
		},
		"AzureSqlDatabaseEmpLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseEmpLinkedService'"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseSampleDatabaseLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sourcestorageaccount789.dfs.core.windows.net/"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ramanaworkspace.sql.azuresynapse.net"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ramanadedicatedsqlpool"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlserver987.database.windows.net"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureSQLDatabaseEmp"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminram"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlserver987.database.windows.net"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mySampleDatabase"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminram"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactory-Battula_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_accountKey": {
						"value": "[parameters('ADLSGen2_accountKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureBlobStorageMovies_connectionString": {
						"value": "[parameters('AzureBlobStorageMovies_connectionString')]"
					},
					"AzureSqlDWLinkedService_password": {
						"value": "[parameters('AzureSqlDWLinkedService_password')]"
					},
					"AzureSqlDatabaseEmpLinkedService_password": {
						"value": "[parameters('AzureSqlDatabaseEmpLinkedService_password')]"
					},
					"AzureSqlDatabaseSampleDatabaseLinkedService_password": {
						"value": "[parameters('AzureSqlDatabaseSampleDatabaseLinkedService_password')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"AzureSqlDWLinkedService_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDWLinkedService_properties_typeProperties_server')]"
					},
					"AzureSqlDWLinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDWLinkedService_properties_typeProperties_database')]"
					},
					"AzureSqlDWLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDWLinkedService_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseEmpLinkedService_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseEmpLinkedService_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseEmpLinkedService_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataFactory-Battula_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataFactory-Battula_ArmTemplate_0')]"
			]
		}
	]
}