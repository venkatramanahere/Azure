		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sourcestorageaccount789.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlserver987.database.windows.net"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureSQLDatabaseEmp"
		},
		"AzureSqlDatabaseEmpLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminram"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mysqlserver987.database.windows.net"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mySampleDatabase"
		},
		"AzureSqlDatabaseSampleDatabaseLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminram"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ramanaworkspace.sql.azuresynapse.net"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ramanadedicatedsqlpool"
		},
		"AzureSqlDWLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		}
	},
	"variables": {
